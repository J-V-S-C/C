/*- Realize a soma de dois elementos. Faça isso através de uma função, a qual será chamada
na função principal.*/

#include<stdio.h>



int soma(int a , int b)
{
int c;
c=a+b;
return c;
}


main()
{

int m, n, k;



printf("Digite um número\n");
scanf("%d", &n);

printf("Digite um número\n");
scanf("%d", &m);


k=soma(m, n);

printf("%d", k);


return(0);
}
